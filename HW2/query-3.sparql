PREFIX ex: <http://example.org/vocabulary/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

# This query retrieves students' names, their grades, and the courses they are enrolled in.
SELECT ?studentName ?courseTitle ?grade
WHERE {
  ?enrollment a ex:Enrollment ;
              ex:grade ?grade ;
              ex:course ?course .
  ?student a ex:Student ;
           foaf:name ?studentName ;
           ex:enrolls ?course .
  ?course ex:title ?courseTitle .
}
