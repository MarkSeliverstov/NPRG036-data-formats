PREFIX ex: <http://example.org/vocabulary/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

# This query retrieves the names of teachers and the courses they teach.
SELECT ?teacherName ?courseTitle
WHERE {
  ?teacher a ex:Teacher ;
           foaf:name ?teacherName ;
           ex:teaches ?course .
  ?course ex:title ?courseTitle .
}