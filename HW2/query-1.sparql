PREFIX ex: <http://example.org/vocabulary/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

# This query retrieves the names of students enrolled in the "CS101" course.
SELECT ?studentName
WHERE {
  ?student a ex:Student ;
           foaf:name ?studentName ;
           ex:enrolls ?course .
  ?course ex:code "CS101" .
}